{
  "name": "react-workshop-starter",
  "version": "1.0.0",
  "description": "React workshop project boilerplate",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "build": "BABEL_ENV=production webpack",
    "lint": "eslint src/*",
    "typecheck": "flow check",
    "test": "BABEL_ENV=test mocha --require babel-register src/**/*-test.js",
    "test:watch": "BABEL_ENV=test mocha --require babel-register src/**/*-test.js --watch --reporter min"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danecando/react-workshop-starter.git"
  },
  "keywords": [
    "React",
    "Workshop",
    "Boilerplate",
    "UXDevSummit",
    "Redux"
  ],
  "author": "Dane Grant",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/danecando/react-workshop-starter/issues"
  },
  "homepage": "https://github.com/danecando/react-workshop-starter#readme",
  "dependencies": {
    "react": "^0.14.6",
    "react-dom": "^0.14.6"
  },
  "devDependencies": {
    "babel-core": "^6.4.0",
    "babel-eslint": "^5.0.0-beta6",
    "babel-loader": "^6.2.1",
    "babel-plugin-react-transform": "^2.0.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-hmre": "^1.0.1",
    "babel-preset-stage-2": "^6.3.13",
    "babel-register": "^6.3.13",
    "browser-sync": "^2.11.0",
    "chai": "^3.4.1",
    "connect-history-api-fallback": "^1.1.0",
    "enzyme": "^1.2.0",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^3.1.0",
    "eslint-plugin-react": "^3.14.0",
    "mocha": "^2.3.4",
    "react-addons-test-utils": "^0.14.6",
    "react-transform-hmr": "^1.0.1",
    "webpack": "^1.12.10",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-hot-middleware": "^2.6.0"
  }
}
